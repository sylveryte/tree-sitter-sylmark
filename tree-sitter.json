{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "sylmark",
      "camelcase": "Sylmark",
      "title": "Sylmark",
      "scope": "source.sylmark",
      "file-types": [
        "markdown,markdown-inline"
      ],
      "injection-regex": "^sylmark$",
      "class-name": "TreeSitterSylmark"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Markdown parser for sylmark lsp",
    "authors": [
      {
        "name": "Sandeep Sahani",
        "email": "sylveryte@protonmail.com",
        "url": "https://sahani.dev/"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-sylmark"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}