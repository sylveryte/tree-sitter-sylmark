{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "sylmark",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "heading"
          },
          {
            "type": "SYMBOL",
            "name": "link"
          },
          {
            "type": "SYMBOL",
            "name": "wikilink"
          },
          {
            "type": "SYMBOL",
            "name": "tag"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "inline_code"
          },
          {
            "type": "SYMBOL",
            "name": "fenced_code_block"
          },
          {
            "type": "SYMBOL",
            "name": "text"
          }
        ]
      }
    },
    "title": {
      "type": "PATTERN",
      "value": "[^\\n\\r]*"
    },
    "heading": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[#]{1,6}"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\s+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "title",
          "content": {
            "type": "SYMBOL",
            "name": "title"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "\n"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "url": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^)\\n]"
            },
            {
              "type": "STRING",
              "value": "\\)"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^)\\n]"
              },
              {
                "type": "STRING",
                "value": "\\)"
              }
            ]
          }
        }
      ]
    },
    "urltext": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^\\]]"
            },
            {
              "type": "STRING",
              "value": "\\]"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^\\]]"
              },
              {
                "type": "STRING",
                "value": "\\]"
              }
            ]
          }
        }
      ]
    },
    "link": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "text",
          "content": {
            "type": "SYMBOL",
            "name": "urltext"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "FIELD",
          "name": "url",
          "content": {
            "type": "SYMBOL",
            "name": "url"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "wikitarget": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^]\\n]"
            },
            {
              "type": "STRING",
              "value": "\\]"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^]\\n]"
              },
              {
                "type": "STRING",
                "value": "\\]"
              }
            ]
          }
        }
      ]
    },
    "wikilink": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "[["
        },
        {
          "type": "FIELD",
          "name": "target",
          "content": {
            "type": "SYMBOL",
            "name": "wikitarget"
          }
        },
        {
          "type": "STRING",
          "value": "]]"
        }
      ]
    },
    "tag": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": "[a-zA-Z0-9_]+"
          }
        ]
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "<!--[\\s\\S]*?-->"
        }
      }
    },
    "inline_code": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "`[^`\\n]*`"
        }
      }
    },
    "fenced_code_block": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "```[\\s\\S]*?```"
        }
      }
    },
    "text": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^\\s#\\[\\]\\[\\]`<]+"
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s+"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}